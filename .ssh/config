Host github.com
	IdentityFile ~/.ssh/git_rsa
Host curie
	HostName curie.eecs.umich.edu
	User victorbp
Host steinmetz
	HostName steinmetz.eecs.umich.edu
	User victorbp

Host *
    Port 22
    IgnoreUnknown AddKeysToAgent,UseKeychain
    UseKeychain yes
    AddKeysToAgent yes
    IdentityFile ~/.ssh/id_rsa
    ControlMaster auto
    ControlPath ~/.ssh/_%r@%h:%p
    ControlPersist 43200
    SetEnv TERM=xterm
